<div id="fiscalYearLineModal" class="modal fade bs-example-modal-lg" data-keyboard="false" role="dialog" aria-labelledby="modal_title" aria-hidden="true" onkeydown="modal_onkeydown(event, this)">
    <div class="modal-dialog modal-size-lg">
        <div class="modal-content">
            <div class="modal-header bg-header">
                <div class="text-right">
                    <h5 class="modal-title mt-0" id="modal_title">ماه‌های دوره مالی</h5>
                </div>
            </div>
            <div class="modal-body text-right p-0">
                <div class="card-body " id="fiscalyearline_pagetable" data-parsley-validate>
                    <div class="d-flex float-right">
                        <h6 class="mt-0 font-15 border-left pl-2 ml-2">نام دوره : <span id="fy_yearTitle"></span></h6>
                        <h6 class="mt-0 font-15 border-left pl-2 ml-2">از : <span id="fy_startDateTitle"></span></h6>
                        <h6 class="mt-0 font-15">منتهی به : <span id="fy_endDateTitle"></span></h6>
                    </div>
                    @await Html.PartialAsync("/Modules/NewPageTable.cshtml")
                </div>
            </div>
            <div class="modal-footer">
                <button id="fiscalYear-close" onclick="modal_close('fiscalYearLineModal')" class="btn btn-secondary waves-effect"><i class="fa fa-times-circle"></i>انصراف</button>
            </div>
        </div>
    </div>
</div>

<script src="/Modules/GN/FiscalYearLine/FiscalYearLineIndex.js"></script>