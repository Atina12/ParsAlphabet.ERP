@{
    ViewBag.Title = "اشخاص";
}


@await Html.PartialAsync("/Modules/NewFormPlate1V1.cshtml")
<!--Modal-->
<div id="AddEditModal" class="modal fade" data-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="modal_title" aria-hidden="true" onkeydown="modal_onkeydown(event, this)">
<div class="modal-dialog modal-size-lg">
<div class="modal-content">
<div class="modal-header bg-header">
    <div class="text-right">
        <h5 class="modal-title mt-0" id="modal_title">افزودن ویرایش</h5>
    </div>
</div>
<div class="modal-body modal-max-height text-right over-visible" data-parsley-validate>
<div class="row" id="rowKeyId">
    <div class="d-block w-100 text-right mb-2 pr-3">
        <div class="key_id">
            <lable id="modal_keyid_caption"></lable>
            <span id="modal_keyid_value"></span>
        </div>
    </div>
</div>
<div class="row">
    <div class="form-group col-sm-4">
        <label>نوع شخصیت</label>
        <div>
            <select id="partnerTypeId" class="form-control" tabindex="1" oninput="checkContactInput(event)" required>
                <option value="1">حقیقی</option>
                <option value="2">حقوقی</option>
            </select>
        </div>
    </div>
    @*<div class="form-group col-sm-4">
                            <label>گروه</label>
                            <div>
                                <select id="groupId" class="form-control" tabindex="1"></select>
                            </div>
                        </div>*@
    <div class="form-group col-sm-4">
        <label>گروه اشخاص</label>
        <div>
            <select id="personGroupId" class="form-control select2" tabindex="2" onchange="checkContactInput(event)" required data-parsley-selectvalzero data-parsley-errors-container="#personGroupIdErrorContainer">
                <option value="0">انتخاب کنید</option>
            </select>
        </div>
        <div id="personGroupIdErrorContainer"></div>
    </div>
    <div class="form-group col-sm-4">
        <label>گروه صنعت</label>
        <div>
            <select id="industryId" class="form-control" tabindex="3">
            </select>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>عنوان</label>
        <div>
            <select id="personTitleId" class="form-control select2" tabindex="4">
                <option value="0">انتخاب نمایید</option>
            </select>
        </div>
    </div>
    <div class="form-group col-sm-4 indiv">
        <label>نام</label>
        <div>
            <input id="firstName" type="text" class="form-control atoz-valid" oninput="checkContactInput(event)" required maxlength="50" title="" tabindex="5"/>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label id="partnerName">نام شرکت</label>
        <div>
            <input id="lastName" type="text" class="form-control" oninput="checkContactInput(event)" required maxlength="50" title="" tabindex="6">
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>نام نماینده</label>
        <div>
            <input id="agentFullName" type="text" class="form-control atoz-valid" maxlength="50" title="" tabindex="7">
        </div>
    </div>
    <div class="form-group col-sm-4 indiv">
        <label>نام تجاری</label>
        <div>
            <input id="brandName" type="text" class="form-control" maxlength="50" title="" tabindex="8">
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>نام جستجو</label>
        <div>
            <input id="searchName" type="text" class="form-control" title="" maxlength="50" tabindex="9"/>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>شماره </label>
        <div>
            <input id="idNumber" type="text" class="form-control str-number" title="" maxlength="11" tabindex="10"/>
        </div>
    </div>
    <div class="form-group col-sm-4" id="jobTitleContainer">
        <label>عنوان شغلی</label>
        <div>
            <input id="jobTitle" type="text" class="form-control" maxlength="50" title="" tabindex="11">
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>تاریخ </label>
        <div>
            <input id="idDatePersian" type="text" class="form-control number persian-date" placeholder="____/__/__" data-inputmask="'mask':'9999/99/99'" data-parsley-shamsidate data-parsley-errors-container="#dateToErrorContainer" title="" tabindex="12"/>
            <div id="dateToErrorContainer"></div>
        </div>
    </div>
    <div class="form-group col-sm-4 indiv">
        <label>جنسیت</label>
        <div>
            <select id="genderId" class="form-control" title="" oninput="checkContactInput(event)" required tabindex="13">
                <option value="1">مرد</option>
                <option value="2">زن</option>
            </select>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>کد </label>
        <div>
            <input id="nationalCode" type="text" class="form-control str-number" title="" maxlength="10" oninput="checkContactInput(event)" data-parsley-existnationalcode tabindex="14" data-parsley-nationalcode/>
        </div>
    </div>
    <div class="form-group col-sm-4 comp">
        <label>کداقتصادی</label>
        <div>
            <input id="taxCode" type="text" class="form-control" maxlength="16" title="" tabindex="15">
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>گروه تخفیف مشتریان</label>
        <div>
            <select id="discGroupId" class="form-control" title="" tabindex="16">
                <option value="0">انتخاب کنید</option>
            </select>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>ملیت</label>
        <div>
            <select id="locCountryId" class="form-control" title="" tabindex="17" data-parsley-errors-container="#locCountryIdErrorContainer"></select>
        </div>
        <div id="locCountryIdErrorContainer"></div>
    </div>
    <div class="form-group col-sm-4">
        <label>ولایت</label>
        <div>
            <select id="locStateId" class="form-control" title="" tabindex="18" data-parsley-errors-container="#locStateIdErrorContainer">
                <option value="0">انتخاب کنید</option>
            </select>
        </div>
        <div id="locStateIdErrorContainer"></div>
    </div>
    <div class="form-group col-sm-4">
        <label>شهر</label>
        <div>
            <select id="locCityId" class="form-control" title="" tabindex="19" data-parsley-errors-container="#locCityIdErrorContainer"></select>
        </div>
        <div id="locCityIdErrorContainer"></div>
    </div>
    <div class="form-group col-sm-4">
        <label>نمبر خانه (Postal Code)</label>
        <div>
            <input id="postalCode" type="text" class="form-control str-number" title="" maxlength="10" data-parsley-postalcode="" tabindex="20"/>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>شماره تلفن</label>
        <div>
            <input id="phoneNo" type="text" class="form-control str-number" title="" maxlength="11" tabindex="21"/>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>شماره موبایل</label>
        <div>
            <input id="mobileNo" type="text" class="form-control str-number" title="" maxlength="11" tabindex="22"/>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>ایمیل</label>
        <div>
            <input id="email" type="text" class="form-control " title="" maxlength="50" data-parsley-email tabindex="23"/>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>وب سایت</label>
        <div>
            <input id="webSite" type="text" class="form-control" title="" maxlength="50" tabindex="24"/>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>VATمنطقه</label>
        <div>
            <select id="vatAreaId" class="form-control" oninput="checkContactInput(event)" required data-parsley-min="1" tabindex="25"></select>
        </div>
    </div>
    <div class="form-group col-sm-4 ">
        <label>مشمول مالیات بر ارزش افزوده</label>
        <div class="funkyradio funkyradio-success" onkeydown="funkyradio_keydown(event,'vatInclude');" tabindex="26">
            <input id="vatInclude" type="checkbox" onchange="funkyradio_onchange(this)" switch-value="بلی,خیر"/>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>اعتبار مالیات بر ارزش افزوده</label>
        <div class="funkyradio funkyradio-success" onkeydown="funkyradio_keydown(event,'vatEnable');" tabindex="27">
            <input id="vatEnable" type="checkbox" onchange="funkyradio_onchange(this)" switch-value="دارد,ندارد"/>
        </div>
    </div>
    <div class="form-group col-sm-4">
        <label>وضعیت</label>
        <div class="funkyradio funkyradio-success" onkeydown="funkyradio_keydown(event,'isActive');" tabindex="28">
            <input id="isActive" type="checkbox" name="checkbox" onchange="funkyradio_onchange(this)" switch-value="فعال,غیرفعال"/>
        </div>
    </div>
    <div class="form-group col-sm-12">
        <label>آدرس منزل</label>
        <div>
            <input id="address" type="text" class="form-control" title="" maxlength="100" tabindex="29"/>
        </div>
    </div>
</div>
</div>
<div class="modal-footer">
    <button id="modal-save" onclick="modal_save()" class="btn btn-success waves-effect waves-light" tabindex="30" title="Ctrl + S"><i class="fa fa-save"></i>ذخیره</button>
    <button id="modal-close" onclick="modal_close()" class="btn btn-secondary waves-effect" data-dismiss="modal" title="Esc"><i class="fa fa-times-circle"></i>انصراف</button>
</div>
</div>
</div>
</div>
<!--Modal-->


<script src="/Modules/FM/getJsonDataAsync.js"></script>
<script src="/Modules/FM/Finanace.js"></script>
<script src="/Modules/CR/Contact/ContactIndex.js"></script>