@{
    ViewBag.Title = "قیمت گذاری بیمه";
}

<link href="~/lib/ModulesCss/Service.css" rel="stylesheet"/>

<style>
    #services_pagetable .table-responsive-table,
    #items_pagetable .table-responsive-table {
        height: calc(100vh - 250px) !important;
    }

    #medicalItemPrice_getPage .table-responsive-table {
        height: calc(100vh - 310px) !important;
    }
</style>


<div class="content-page displaynone" id="content-page">
    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card m-b-30">
                        <div class="header-title">
                            <div class="button-items col-md-12">
                                <span class="float-right">@ViewBag.Title</span>
                            </div>
                        </div>

                        <div class="card-body" id="medicalItemPrice_getPage">
                            <fieldset class="group-box group-box-gray">
                                <legend>فیلتر</legend>
                                <div class="row slideToggle open" id="medicalItemPriceFilters" data-parsley-validate>
                                    <div class="row col-12">
                                        <div class="form-group col-lg-2 col-sm-4">
                                            <label>شناسه</label>
                                            <input id="medicalItemPriceId" type="text" class="form-control number" tabindex="1"/>
                                        </div>
                                        <div class="form-group col-lg-2 col-sm-4">
                                            <label>دسته بندی کالا / خدمت</label>
                                            <div class="col-sm-12 chosen-container-parent">
                                                <select id="itemTypeId" class="form-control select2" tabindex="2" data-parsley-selectvalzero data-parsley-errors-container="#itemTypeIdErrorContainer">
                                                    <option value="0">انتخاب کنید</option>
                                                    <option value="1">1 - کالا</option>
                                                    <option value="2">2 - خدمت</option>
                                                </select>
                                            </div>
                                            <div id="itemTypeIdErrorContainer"></div>
                                        </div>
                                        <div class="form-group col-lg-2 col-sm-4">
                                            <label>نوع بیمه</label>
                                            <div class="col-sm-12 chosen-container-parent">
                                                <select id="insurerTypeId" class="form-control select2" tabindex="3" data-parsley-selectvalzero data-parsley-errors-container="#insurerTypeIdErrorContainer">
                                                    <option value="0">انتخاب کنید</option>
                                                    <option value="1">1 - اجباری</option>
                                                    <option value="2">2 - تکمیلی</option>
                                                    <option value="4">4 - طرف قرارداد</option>
                                                    <option value="5">5 - تخفیف</option>
                                                </select>
                                            </div>
                                            <div id="insurerTypeIdErrorContainer"></div>
                                        </div>
                                        <div class="form-group col-lg-2 col-sm-4">
                                            <label>موضوع درمان</label>
                                            <div class="col-sm-12 chosen-container-parent">
                                                <select id="medicalSubjectId" class="form-control select2" tabindex="4" data-parsley-errors-container="#medicalSubjectIdErrorContainer">
                                                </select>
                                            </div>
                                            <div id="medicalSubjectIdErrorContainer"></div>
                                        </div>
                                    </div>
                                    <div class="row col-12">
                                        <div class="form-group col-lg-4 col-sm-4">
                                            <label id="itemIdLabel"></label>
                                            <div class="col-sm-12 chosen-container-parent">
                                                <select id="itemId" class="form-control select2" tabindex="5">
                                                    <option value="0">انتخاب کنید</option>
                                                </select>
                                            </div>
                                            <div id="itemIdErrorContainer"></div>
                                        </div>
                                        <div class="form-group col-lg-2 col-sm-4">
                                            <label style="visibility:hidden">جستجو</label>
                                            <div class="col-sm-12">
                                                <div class="button-items text-right">
                                                    <button onclick="getMedicalItemPriceList()" id="getList" class="btn green_2 waves-effect waves-light" tabindex="6"><i class="fa fa-search"></i>جستجو</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                            @await Html.PartialAsync("/Modules/NewPageTable.cshtml")
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="insurerPriceModal" class="modal fade" data-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="modal_title" data-backdrop="static" aria-hidden="true" onkeydown="">
    <div class="modal-dialog modal-size-xl">
        <div class="modal-content">
            <div class="modal-header bg-header">
                <div class="text-right">
                    <h5 class="modal-title mt-0">تعرفه بیمه کالا و خدمات</h5>
                </div>
            </div>
            <div class="modal-body modal-max-height text-right" id="insurerPriceForm" data-parsley-validate>
                <div class="row">
                    <div class="form-group col-sm-4">
                        <label>کالا/خدمات</label>
                        <div>
                            <select id="insurerPriceItemTypeId" class="form-control select2" tabindex="29">
                                <option value="1">1- کالا</option>
                                <option value="2">2 - خدمات</option>
