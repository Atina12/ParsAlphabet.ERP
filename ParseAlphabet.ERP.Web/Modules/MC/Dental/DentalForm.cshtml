@{
    ViewBag.Title = "دندانداکتری";
}

<link href="~/lib/jquery-ui/css/jquery-ui.css" rel="stylesheet"/>


<div class="content-page displaynone" id="content-page">
<div class="content" id="mainDentalFormBox">
<div class="container-fluid">
<div class="row">
<div class="col-12">
<div class="card m-b-30 m-t-15">
<div class="header-title d-flex">
    <span>@ViewBag.Title</span>
    <div class="button-items">
        <button id="choiceOfAdmission" type="button" onclick="modal_show('searchAdmissionModal');" class="btn btn-info waves-effect" title="Ctrl + F"><i class="fa fa-user-alt"></i>انتخاب پذیرش</button>
        <button id="list_adm" type="button" class="btn btn_green_1 waves-effect"><i class="fa fa-list-ul"></i>لیست</button>
        <button id="saveForm" type="button" onclick="saveDentalForm()" class="btn btn-success waves-effect" title="Ctrl + S"><i class="fa fa-save"></i>ذخیره</button>
        <button id="newForm" type="button" onclick="resetFormDental()" class="btn btn-info waves-effect"><i class="fa fa-plus-circle"></i>جدید</button>
    </div>
</div>
<div class="card-body" data-parsley-validate>
<div class="row displaynone" id="BoxBox">
    <div class="col-12 d-flex">
        <div class="form-group col-lg-1 col-sm-2">
            <label>شناسه</label>
            <div>
                <input id="dentalId" type="text" class="form-control number" maxlength="10" readonly value="@ViewContext.RouteData.Values["id"]"/>
            </div>
        </div>
        <div class="form-group col-lg-2 col-sm-4">
            <label>ثبت کننده</label>
            <input id="userFullName" type="text" class="form-control" readonly/>
        </div>
    </div>
</div>

<fieldset class="group-box group-box-orange mt-3">
    <legend>مراجعه کننده</legend>
    <table class="table">
        <thead>
        <tr>
            <th>شناسه</th>
            <th>مراجعه کننده</th>
            <th>نمبرتذکره</th>
            <th>بیمه پایه</th>
            <th>صندوق</th>
            <th>بیمه تکمیلی</th>
            <th>طرف قرارداد</th>
            <th>شناسه شباد</th>
            <th>انقضاء دفترچه</th>
            <th>داکتر</th>
            <th>عملیات</th>
        </tr>
        </thead>
        <tbody id="admissionSelected">
        </tbody>
    </table>
</fieldset>

<fieldset class="group-box group-box-gray mt-3" id="sendReferFieldset">
<legend>اطلاعات دندانداکتری</legend>

<ul class="nav nav-pills nav-justified" role="tablist">
    <li class="nav-item waves-effect waves-light" onclick="focusInput(1)">
        <a class="nav-link active" data-toggle="tab" id="dentalFormLink" href="#dentalFormBox" role="tab">
            <span class="d-none d-md-block">مشخصات دندان</span>
            <span class="d-block d-md-none">
                <i class="mdi mdi-account h5"></i>
            </span>
        </a>
    </li>
    <li class="nav-item waves-effect waves-light" onclick="focusInput(2)">
        <a class="nav-link" data-toggle="tab" id="abuseHistoryLink" href="#abuseHistoryBox" role="tab">
            <span class="d-none d-md-block">مصرف دخانیات</span>
            <span class="d-block d-md-none">
                <i class="mdi mdi-account h5"></i>
            </span>
        </a>
    </li>
    <li class="nav-item waves-effect waves-light" onclick="focusInput(3)">
        <a class="nav-link" data-toggle="tab" id="familyHistoryLink" href="#familyHistoryBox" role="tab">
            <span class="d-none d-md-block">سوابق خانوادگی</span>
            <span class="d-block d-md-none">
                <i class="mdi mdi-email h5"></i>
            </span>
        </a>
    </li>
    <li class="nav-item waves-effect waves-light" onclick="focusInput(4)">
        <a class="nav-link" data-toggle="tab" id="drugHistoryLink" href="#drugHistoryBox" role="tab">
            <span class="d-none d-md-block">تاریخچه دارویی</span>
            <span class="d-block d-md-none">
                <i class="mdi mdi-email h5"></i>
            </span>
        </a>
    </li>
    <li class="nav-item waves-effect waves-light" onclick="focusInput(5)">
        <a class="nav-link" data-toggle="tab" id="drugOrderedLink" href="#drugOrderedBox" role="tab">
            <span class="d-none d-md-block">ویژگی دارو تجویز شده</span>
            <span class="d-block d-md-none">
                <i class="mdi mdi-email h5"></i>
            </span>
        </a>
    </li>
    <li class="nav-item waves-effect waves-light" onclick="focusInput(6)">
        <a class="nav-link" data-toggle="tab" id="adverseReactionLink" href="#adverseReactionBox" role="tab">
            <span class="d-none d-md-block">عوارض جانبی</span>
            <span class="d-block d-md-none">
                <i class="mdi mdi-email h5"></i>
            </span>
        </a>
    </li>
    <li class="nav-item waves-effect waves-light" onclick="focusInput(7)">
        <a class="nav-link" data-toggle="tab" id="medicalHistoryLink" href="#medicalHistoryBox" role="tab">
            <span class="d-none d-md-block">تاریخچه داکتری مراجعه کننده</span>
            <span class="d-block d-md-none">
                <i class="mdi mdi-email h5"></i>
            </span>
        </a>
    </li>
    <li class="nav-item waves-effect waves-light" onclick="focusInput(8)">
        <a class="nav-link" data-toggle="tab" id="diagLink" href="#diagBox" role="tab">
            <span class="d-none d-md-block">تشخیص</span>
            <span class="d-block d-md-none">
                <i class="mdi mdi-email h5"></i>
            </span>
        </a>
    </li>
</ul>
<!-- Tab panes -->
<div class="tab-content group-box group-box-gray">
@*START tooth*@
<div class="tab-pane active p-3" id="dentalFormBox" role="tabpanel">
    <div class="row tabToggle1" id="dentalForm" data-parsley-validate>
        <div class="form-group col-lg-2  col-sm-8">
            <label> دندان کشیدنی</label>
            <div class="funkyradio funkyradio-success" onkeydown="funkyradio_keydown(event,'isMissing');" tabindex="10">
                <input id="isMissing" type="checkbox" onchange="funkyradio_onchange(this)" switch-value="بلی,خیر"/>
            </div>
        </div>
        <div class="form-group col-lg-2  col-sm-4">
            <label>شماره دندان</label>
            <div>
                <select id="toothId" class="form-control select2" tabindex="11" required data-parsley-errors-container="#toothNameIdContainer"></select>
            </div>
            <div id="toothNameIdContainer"></div>
        </div>
        <div class="form-group col-lg-2  col-sm-4">
            <label>بخش دندان</label>
            <div>
                <select id="partId" class="form-control select2" tabindex="12" required data-parsley-errors-container="#partIdContainer"></select>
            </div>
            <div id="partIdContainer"></div>
        </div>
        <div class="form-group col-lg-2  col-sm-4">
            <label>سطح دندان</label>
            <div>
                <select id="segmentId" class="form-control select2" tabindex="13" required data-parsley-errors-container="#segmentIdContainer"></select>
            </div>
            <div id="segmentIdContainer"></div>
        </div>
        <div class="form-group col-lg-4  col-sm-4">
            <label>توضیحات</label>
            <div>
                <input id="toothComment" type="text" class="form-control" required maxlength="100" tabindex="14"/>
            </div>
        </div>
        @*//*@
        <div style="visibility:hidden" class="form-group col-lg-1  col-sm-4">
            <label>تشخیص</label>
            <div>
                <input id="hasDiaganosis" type="text" class="form-control" maxlength="100" tabindex="14"/>
            </div>
        </div>
        <div style="visibility:hidden" class="form-group col-lg-1  col-sm-4">
            <label>خدمت</label>
            <div>
                <input id="hasTreatment" type="text" class="form-control" maxlength="100" tabindex="14"/>
            </div>
        </div>
        <div class="col-sm text-right">
            <div class="button-items" style="margin:5px 0 10px 10px;">
                <button type="button" id="addTooth" class="btn btn-light border-green my-2" tabindex="15"><i class="fa fa-arrow-down"></i>ثبت</button>
                <button id="canceledTooth" type="button" class="btn btn-light border-green" tabindex="16"><i class="fa fa-times"></i>&nbsp;&nbsp;انصراف</button>
            </div>
        </div>

        <div class="table-responsive" style="width:100%;height: 300px;">
            <table class="table table-styles border mb-0" style="table-layout:fixed;font-size:10px" id="tempToothList">
                <thead>
                <tr>
                    <th class="col-width-percent-5">ردیف</th>
                    <th class="col-width-percent-10">دندان ازدست رفته</th>
                    <th>شماره دندان</th>
                    <th>بخش دندان</th>
                    <th>سطح دندان </th>
                    <th>توضیحات</th>
                    <th>تشخیص</th>
                    <th>خدمات</th>
                    <th>عملیات</th>
                </tr>
                </thead>
                <tbody id="tempTooth">
                <tr id="emptyRow">
                    <td colspan="9" class="text-center">سطری وجود ندارد</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
@*END tooth*@

