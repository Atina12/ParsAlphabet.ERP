    @*END drugHistory*@

    @*START drugOrdered*@
    <div class="tab-pane  p-3" id="drugOrderedBox" role="tabpanel">
        <div class="row tabToggle8" id="drugOrderedForm" data-parsley-validate>
            <div class="table-responsive" style="width:100%;height: 300px;">
                <table class="table table-styles border mb-0" style="table-layout:fixed;font-size:10px" id="tempdrugOrderedList">
