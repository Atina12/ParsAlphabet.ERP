</div>
</fieldset>
</div>
</div>
</div>
</div>
</div> <!-- end col -->
</div>
</div>

@*START diagToothForm*@

<div id="ToothModalC" class="modal fade" data-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="modal_title" aria-hidden="true" onkeydown="modal_onkeydown(event, this)">
    <div class="modal-dialog modal-size-xlg">
        <div class="modal-content">
            <div class="modal-header bg-header">
                <div class="text-right pr-0">
                    <h5 class="modal-title mt-0" id="modal_title"> تشخیص های ثبت شده برای هر دندان</h5>
                </div>
            </div>
            <div class="modal-body modal-max-height text-right">
                <div class="row" style="margin:0 -7px 0 -7px">
                    <fieldset class="group-box group-box-gray" style="overflow-x:hidden;overflow-y:scroll;height:auto;width:100%">
                        <legend>تشخیص های دندان</legend>
                        <div class="table-responsive" style="margin:0 auto;padding-bottom:10px">
                            <table class="table table-styles border mb-0" id="tempToothListC" style="font-size:10px">
                                <thead>
                                <tr>
                                    <th class="col-width-percent-5">ردیف</th>
                                    <th class="col-width-percent-20">وضعیت تشخیص</th>
                                    <th class="col-width-percent-10">تشخیص</th>
                                    <th class="col-width-percent-10">میزان شدت بیماری</th>
                                    <th class="col-width-percent-10">توضیحات</th>
                                </tr>
                                </thead>
                                <tbody id="tempToothC">
                                <tr id="emptyRow">
                                    <td colspan="5" class="text-center">سطری وجود ندارد</td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </fieldset>
                </div>
            </div>
            <div class="modal-footer">
                <button id="modalCloseToothC" class="btn btn-secondary waves-effect " title="Esc"><i class="fa fa-times-circle"></i>بستن</button>
            </div>
        </div>
    </div>
</div>

<div id="TreatmentModalC" class="modal fade" data-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="modal_title" aria-hidden="true" onkeydown="modal_onkeydown(event, this)">
    <div class="modal-dialog modal-size-xlg">
        <div class="modal-content">
            <div class="modal-header bg-header">
                <div class="text-right pr-0">
                    <h5 class="modal-title mt-0" id="modal_title">خدمات بیمار</h5>
                </div>
            </div>
            <div class="modal-body modal-max-height text-right">
                @*<div class="row" id="treatmentBoxC" data-parsley-validate>
                    </div>*@

                <div class="row" style="margin:0 -7px 0 -7px">
                    <fieldset class="group-box group-box-gray" style="overflow-x:hidden;overflow-y:scroll;height:auto;width:100%">
                        <legend>خدمات بیمار</legend>
                        <div class="table-responsive" style="margin:0 auto;padding-bottom:10px">
                            <table class="table table-styles border mb-0" id="tempTreatmentListC" style="font-size:10px">
                                <thead>
                                <tr>
                                    <th class="col-width-percent-5">ردیف</th>
                                    <th class="col-width-percent-20">نوع خدمت</th>
                                    <th class="col-width-percent-10">خدمت ارائه شده</th>
                                    <th class="col-width-percent-10">تعداد خدمت</th>
                                    <th class="col-width-percent-5">واحد خدمت</th>
                                </tr>
                                </thead>
                                <tbody id="tempTreatmentC">
                                <tr id="emptyRow">
                                    <td colspan="5" class="text-center">سطری وجود ندارد</td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </fieldset>
                </div>
            </div>
            <div class="modal-footer">
                <button id="modalCloseTreatmentC" class="btn btn-secondary waves-effect "><i class="fa fa-times-circle"></i>بستن</button>
            </div>
        </div>
    </div>
</div>


