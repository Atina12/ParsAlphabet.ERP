@{ ViewBag.Title = "قیمت گذاری کالا و خدمات"; }
<link href="~/lib/ModulesCss/Service.css" rel="stylesheet"/>
<link href="~/lib/ModulesCss/serviceItemPricing.css" rel="stylesheet"/>

<div class="content-page displaynone" id="content-page">
    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card m-b-30">
                        <div class="header-title">
                            <div class="button-items col-md-12">
                                <span class="float-right">@ViewBag.Title</span>
                            </div>
                        </div>
                        <div class="card-body">
                            <ul id="serviceItemPricingTabs" class="nav nav-pills nav-justified" role="tablist">
                                <li class="nav-item waves-effect waves-light" onclick="changeTabInbound(1,'services_pagetable')">
                                    <a class="nav-link active " data-toggle="tab" href="#services" role="tab">
                                        <div class="d-flex justify-content-center align-items-center w-100" style="font-size: 11px!important;">
                                            <div class="">خدمات</div>
                                        </div>
                                    </a>
                                </li>
                                <li class="nav-item waves-effect waves-light" onclick="changeTabInbound(2,'items_pagetable')">
                                    <a class="nav-link" data-toggle="tab" href="#items" role="tab">
                                        <div class="d-flex justify-content-center align-items-center w-100" style="font-size: 11px!important;">
                                            <div class="">کالا</div>
                                        </div>
                                    </a>
                                </li>
                            </ul>
                            <div class="tab-content group-box group-box-gray" @*style="display:none"*@>
                                <div class="tab-pane active pr-3 pl-3 pb-3 pt-0" id="services" role="tabpanel">
                                    <div class="row tabToggle1" data-parsley-validate>
                                        <div class="col-12">
                                            <div class="card">
                                                <div class="card-body" id="services_pagetable">
                                                    @await Html.PartialAsync("/Modules/NewPageTableV1.cshtml")
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="tab-pane pr-3 pl-3 pb-3 pt-0" id="items" role="tabpanel">
                                    <div class="row tabToggle2">
                                        <div class="col-12">
                                            <div class="card">
                                                <div class="card-body" id="items_pagetable">
                                                    @await Html.PartialAsync("/Modules/NewPageTableV1.cshtml")
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="servicePricingModal" class="modal fade bs-example-modal-lg" data-keyboard="false" role="dialog" aria-labelledby="modal_title" aria-hidden="true" onkeydown="modal_onkeydown(event, this)">
    <div class="modal-dialog modal-size-xlg">
        <div class="modal-content">
            <div class="modal-header bg-header">
                <div class="text-right">
                    <h5 class="modal-title mt-0">قیمت گذاری خدمت</h5>
                </div>
            </div>
            <div class="modal-header" id="">
                <div class="items-info text-right d-flex">
                    <div class="services-info text-right d-flex">
                        <p class="d-inline-block pl-2 m-1 border-left font-12">
                            <span class="serviceTypeOfpricing"></span>
                        </p>
                        <p class="d-inline-block pl-2 m-1 border-left font-12">کد خدمت : <span class="serviceCodeModal"></span></p>
                        <p class="d-inline-block pl-2 m-1 border-left font-12">نام خدمت : <span class="serviceNameModal"></span></p>
                        <p class="d-inline-block pl-2 m-1 font-12">نوع خدمت : <span class="serviceTypeModal"></span></p>
                    </div>

                </div>
            </div>
            <div class="modal-body text-right p-0">
                <div class="card-body " id="servicePricing_pagetable" data-parsley-validate>
                    @await Html.PartialAsync("/Modules/NewPageTableV1.cshtml")
                </div>
            </div>
            <div class="modal-footer">
                <button id="modal-close" onclick="modal_close('servicePricingModal')" class="btn btn-secondary waves-effect" title="Esc"><i class="fa fa-times-circle"></i>انصراف</button>
            </div>
        </div>
    </div>
</div>

<div id="itemPricingModal" class="modal fade bs-example-modal-lg" data-keyboard="false" role="dialog" aria-labelledby="modal_title" aria-hidden="true" onkeydown="modal_onkeydown(event, this)">
    <div class="modal-dialog modal-size-xxxlg">
        <div class="modal-content">
            <div class="modal-header bg-header">
                <div class="text-right">
                    <h5 class="modal-title mt-0">قیمت گذاری کالا</h5>
                </div>
            </div>
            <div class="modal-header">
                <div class="items-info text-right d-flex">
                    <p class="d-inline-block pl-2 m-1 border-left font-12">
                        <span class="itemTypeOfpricing"></span>
                    </p>
                    <p class="d-inline-block pl-2 m-1 border-left font-12">شناسه کالا : <span class="itemIdModal"></span></p>
                    <p class="d-inline-block pl-2 m-1 border-left font-12">نام کالا : <span class="itemNameModal"></span></p>
                    <p class="d-inline-block pl-2 m-1 border-left font-12">واحد شمارش : <span class="itemUnitModal"></span></p>
                    <p class="d-inline-block pl-2 m-1 font-12">دسته بندی : <span class="itemCategoryModal"></span></p>
                </div>
            </div>
            <div class="modal-body text-right p-0">
                <div class="card-body " id="itemPricing_pagetable" data-parsley-validate>
                    @await Html.PartialAsync("/Modules/NewPageTableV1.cshtml")
                </div>
            </div>
            <div class="modal-footer">
                <button id="modal-close" onclick="modal_close('itemPricingModal')" class="btn btn-secondary waves-effect" title="Esc"><i class="fa fa-times-circle"></i>انصراف</button>
            </div>
        </div>
    </div>
</div>

<div id="servicePriceAndItemPriceModal" class="modal fade" data-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="modal_title" data-backdrop="static" aria-hidden="true" onkeydown="modal_onkeydown(event, this)">
<div class="modal-dialog modal-size-xl">
<div class="modal-content">
<div class="modal-header bg-header">
    <div class="text-right">
        <h5 class="modal-title mt-0">بروزرسانی تعرفه کالا وخدمات</h5>
    </div>
</div>
<div class="modal-body modal-max-height text-right" id="servicePriceAndItemPriceForm">
    <ul class="nav nav-pills nav-justified" role="tablist">
