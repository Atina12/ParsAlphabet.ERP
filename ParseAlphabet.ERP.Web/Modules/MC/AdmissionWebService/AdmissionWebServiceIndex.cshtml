<div id="getInsurerReimbursementModal" class="modal fade" tabindex="-1" data-keyboard="false" role="dialog" aria-labelledby="modal_title" aria-hidden="true" onkeydown="modal_onkeydown(event, this)">
<div class="modal-dialog modal-size-xxlg">
<div class="modal-content">
<div class="modal-header bg-header">
    <div class="text-right">
        <h5 class="modal-title mt-2" id="">استعلام کسورات بیمه</h5>
    </div>
</div>
<div class="modal-body modal-max-height text-right" data-parsley-validate>
<div class="card-body">
@*compositionField*@
<fieldset class="group-box group-box-gray">
    <legend>اطلاعات پذیرش</legend>
    <ul class="nav nav-pills nav-justified" role="tablist">
        <li class="nav-item waves-effect waves-light">
            <a class="nav-link active" data-toggle="tab" href="#AdmissionFieldBox" role="tab">
                <span class="d-none d-md-block">مراجعه کننده</span>
            </a>
        </li>
        <li class="nav-item waves-effect waves-light">
            <a class="nav-link" data-toggle="tab" href="#diagnosisFieldBox" role="tab">
                <span class="d-none d-md-block">اطلاعات تشخیص</span>
            </a>
        </li>
        <li class="nav-item waves-effect waves-light">
            <a class="nav-link" data-toggle="tab" href="#dischargeFieldBox" role="tab">
                <span class="d-none d-md-block">اطلاعات ترخیص</span>
            </a>
        </li>
        <li class="nav-item waves-effect waves-light">
            <a class="nav-link" data-toggle="tab" href="#insuranceFieldBox" role="tab">
                <span class="d-none d-md-block">اطلاعات بیمه</span>
            </a>
        </li>
        <li class="nav-item waves-effect waves-light">
            <a class="nav-link" data-toggle="tab" href="#reimbursementSummaryFieldBox" role="tab">
                <span class="d-none d-md-block">خلاصه کسورات</span>
            </a>
        </li>
        <li class="nav-item waves-effect waves-light">
            <a class="nav-link" data-toggle="tab" href="#reimbursementServicesFieldBox" role="tab">
                <span class="d-none d-md-block">جزئیات کسورات</span>
            </a>
        </li>
    </ul>
    <div class="tab-content group-box group-box-gray">
        <div class="tab-pane active p-3" id="AdmissionFieldBox" role="tabpanel">
            <div class="table-responsive table-responsive-report height-unset">
                <table class="table table-styles border mb-0" style="table-layout:fixed" id="tempAdmissionFieldList">
                    <thead class="table-thead-fixed">
                    <tr>
                        <th>تاریخ پذیرش</th>
                        <th>زمان پذیرش</th>
                        <th>نوع پذیرش</th>
                        <th>تخت</th>
                        <th>داکتر ویزیت</th>
                        <th>داکتر داکتر</th>
                        <th>مرکز درمانی</th>
                        <th>شماره پرونده</th>
                        <th>علت مراجعه</th>
                        <th>داکتر ارجاع دهنده</th>
                    </tr>
                    </thead>
                    <tbody id="tempAdmissionField" class="table-body-report"></tbody>
                </table>
            </div>
        </div>
        <div class="tab-pane p-3" id="diagnosisFieldBox" role="tabpanel">
            <div class="table-responsive table-responsive-report height-unset">
                <table class="table table-styles border mb-0" style="table-layout:fixed" id="tempdiagnosisFieldList">
                    <thead class="table-thead-fixed">
                    <tr>
                        <th>ردیف</th>
                        <th>وضعیت تشخیص</th>
                        <th>تشخیص</th>
                        <th>میزان شدت بیماری </th>
                        <th>توضیحات</th>
                    </tr>
                    </thead>
                    <tbody id="tempdiagnosisField"class="table-body-report"></tbody>
                </table>
            </div>
        </div>
        <div class="tab-pane p-3" id="dischargeFieldBox" role="tabpanel">
            <div class="table-responsive table-responsive-report height-unset">
                <table class="table table-styles border mb-0" style="table-layout:fixed" id="tempdischargeFieldList">
                    <thead class="table-thead-fixed">
                    <tr>
                        <th>تاریخ ترخیص</th>
                        <th>زمان ترخیص</th>
                        <th>وضعیت ترخیص</th>
                    </tr>
                    </thead>
                    <tbody id="tempdischargeField"class="table-body-report"></tbody>
                </table>
            </div>
        </div>
        <div class="tab-pane p-3" id="insuranceFieldBox" role="tabpanel">
            <div class="table-responsive table-responsive-report height-unset">
                <table class="table table-styles border mb-0" style="table-layout:fixed" id="tempinsuranceFieldList">
                    <thead class="table-thead-fixed">
                    <tr>
                        <th>ردیف</th>
                        <th>بیمه گر</th>
                        <th>صندوق بیمه</th>
                        <th>شماره صفحه دفترچه</th>
                        <th>تاریخ انقضا</th>
                        <th>شماره بیمه</th>
                        <th>شباد</th>
                    </tr>
                    </thead>
                    <tbody id="tempinsuranceField"class="table-body-report"></tbody>
                </table>
            </div>
        </div>
        <div class="tab-pane p-3" id="reimbursementSummaryFieldBox" role="tabpanel">
            <div class="table-responsive table-responsive-report height-unset">
                <table class="table table-styles border mb-0" style="table-layout:fixed" id="tempreimbursementSummaryFieldList">
