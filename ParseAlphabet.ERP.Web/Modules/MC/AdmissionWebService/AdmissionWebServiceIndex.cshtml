@{
    ViewBag.Title = "لیست اعتبار کسور بیمه";
}
<link href="~/lib/ModulesCss/AdmissionWebService.css" rel="stylesheet"/>
<link href="~/lib/jquery-ui/css/jquery-ui.css" rel="stylesheet"/>

<div class="content-page displaynone" id="content-page">
<div class="content">
<div class="container-fluid">
    <div class="col-12">
        <div class="card m-b-30 m-t-15">
            <div class="card-body" id="webSreviceFrom">
                <ul class="nav nav-pills nav-justified" role="tablist">
                    <li class="nav-item waves-effect waves-light" onclick="focusInputWebSevice(1,'reimbursment_pagetable')">
                        <a class="nav-link active" data-toggle="tab" href="#admission" role="tab">
                            <span class="d-none d-md-block">پذیرش</span>
                            <span class="d-block d-md-none">
                                <i class="mdi mdi-home-variant h5"></i>
                            </span>
                        </a>
                    </li>
                    <li class="nav-item waves-effect waves-light" onclick="focusInputWebSevice(2,'reimbursment_pagetable2')">
                        <a class="nav-link" data-toggle="tab" href="#returnAdmission" role="tab">
                            <span class="d-none d-md-block">مرجوع</span>
                            <span class="d-block d-md-none">
                                <i class="mdi mdi-account h5"></i>
                            </span>
                        </a>
                    </li>
                    <li class="nav-item waves-effect waves-light" onclick="focusInputWebSevice(3,'reimbursment_pagetable3')">
                        <a class="nav-link" data-toggle="tab" href="#AdmissionRefer" role="tab">
                            <span class="d-none d-md-block">ارجاع</span>
                            <span class="d-block d-md-none">
                                <i class="mdi mdi-account h5"></i>
                            </span>
                        </a>
                    </li>
                    <li class="nav-item waves-effect waves-light" onclick="focusInputWebSevice(4,'reimbursment_pagetable4')">
                        <a class="nav-link" data-toggle="tab" href="#AdmissionFeedBack" role="tab">
                            <span class="d-none d-md-block">بازخورد</span>
                            <span class="d-block d-md-none">
                                <i class="mdi mdi-account h5"></i>
                            </span>
                        </a>
                    </li>
                    <li class="nav-item waves-effect waves-light" onclick="focusInputWebSevice(5,'prescreption_pagetable')">
                        <a class="nav-link" data-toggle="tab" href="#prescreption" role="tab">
                            <span class="d-none d-md-block">نسخه نویسی</span>
                            <span class="d-block d-md-none">
                                <i class="mdi mdi-account h5"></i>
                            </span>
                        </a>
                    </li>
                    <li class="nav-item waves-effect waves-light" onclick="focusInputWebSevice(6,'dental_pagetable')">
                        <a class="nav-link" data-toggle="tab" href="#dental" role="tab">
                            <span class="d-none d-md-block">دندانداکتری</span>
                            <span class="d-block d-md-none">
                                <i class="mdi mdi-account h5"></i>
                            </span>
                        </a>
                    </li>
                    <li class="nav-item waves-effect waves-light" onclick="focusInputWebSevice(7,'death_pagetable')">
                        <a class="nav-link" data-toggle="tab" href="#death" role="tab">
                            <span class="d-none d-md-block">فوت</span>
                            <span class="d-block d-md-none">
                                <i class="mdi mdi-account h5"></i>
                            </span>
                        </a>
                    </li>
                    <li class="nav-item waves-effect waves-light" onclick="focusInputWebSevice(8,'medicallaboratory_pagetable')">
                        <a class="nav-link" data-toggle="tab" href="#medical" role="tab">
                            <span class="d-none d-md-block">آزمایشگاه</span>
                            <span class="d-block d-md-none">
                                <i class="mdi mdi-account h5"></i>
                            </span>
                        </a>
                    </li>
                </ul>
                <div class="tab-content group-box group-box-gray" style="display:none">
                    <div class="tab-pane active pr-3 pl-3 pb-3 pt-0" id="admission" role="tabpanel">
                        <div class="row tabToggle1" data-parsley-validate>
                            <div class="col-12">
                                <div class="header-title d-flex pb-4">
                                    <span>لیست پذیرش</span>
                                    <div class="button-items">
                                        <button type="button" id="startTaskUpdateHID" class="btn green_4 waves-effect"><i id="updateHidBtnLoading" class="fa fa-save"></i>بروزرسانی شباد خودکار</button>
                                        <button type="button" id="updateHidBtn" class="btn green_4 waves-effect"><i id="updateHidBtnLoading" class="fa fa-save"></i>بروز رسانی شباد</button>
                                        <button type="button" id="savePatientBillBtn" class="btn btn_green_1 waves-effect"><i id="savePatientBillBtnLoading" class="fa fa-save"></i>ارسال روکش اسناد</button>
                                        <button type="button" id="getInsurerReimbursementBtn" class="btn btn-info waves-effect waves-light"><i id="getInsurerReimbursementBtnLoading" class="fa fa-save"></i>استعلام کسورات بیمه</button>
                                    </div>
                                </div>
                                <div class="card-body " id="reimbursment_pagetable">
                                    @await Html.PartialAsync("/Modules/NewPageTable.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane pr-3 pl-3 pb-3 pt-0" id="returnAdmission" role="tabpanel">
                        <div class="row tabToggle2">
                            <div class="col-12">
                                <div class="header-title d-flex pb-4">
                                    <span>لیست مرجوعی</span>
                                    <div class="button-items">
                                        <button type="button" id="eliminateHidBtn" class="btn btn-orange waves-effect"><i class="fa fa-save"></i>ابطال پذیرش</button>
                                    </div>
                                </div>
                                <div class="card-body " id="reimbursment_pagetable2">
                                    @await Html.PartialAsync("/Modules/NewPageTable.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="tab-pane pr-3 pl-3 pb-3 pt-0" id="AdmissionRefer" role="tabpanel">
                        <div class="row tabToggle2">
                            <div class="col-12">
                                <div class="header-title d-flex pb-4">
                                    <span>لیست ارجاع</span>
                                    <div class="button-items">
                                        <button type="button" id="sendReferallBtn" class="btn btn-success waves-effect"><i class="fa fa-save"></i>ارسال ارجاع</button>
                                    </div>
                                </div>
                                <div class="card-body " id="reimbursment_pagetable3" style="min-height: calc(100vh - 384px);">
                                    @await Html.PartialAsync("/Modules/NewPageTable.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane pr-3 pl-3 pb-3 pt-0" id="AdmissionFeedBack" role="tabpanel">
                        <div class="row tabToggle4">
                            <div class="col-12">
                                <div class="header-title d-flex pb-4">
                                    <span>لیست بازخورد</span>
                                    <div class="button-items">
                                        <button type="button" id="sendFeedBackBtn" class="btn btn-info waves-effect waves-light"><i class="fa fa-save"></i>ارسال بازخورد</button>
                                    </div>
                                </div>
                                <div class="card-body " id="reimbursment_pagetable4" style="min-height: calc(100vh - 384px);">
                                    @await Html.PartialAsync("/Modules/NewPageTable.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane pr-3 pl-3 pb-3 pt-0" id="prescreption" role="tabpanel">
                        <div class="row tabToggle5">
                            <div class="col-12">
                                <div class="header-title d-flex pb-4">
                                    <span>لیست نسخه نویسی</span>
                                    <div class="button-items">
                                        <button type="button" id="prescriptionGetHidBtn" class="btn green_4 waves-effect"><i id="prescriptionGetHidBtnLoading" class="fa fa-save"></i>دریافت شباد</button>
                                        <button type="button" id="sendPrescriptionBtn" class="btn btn-info waves-effect waves-light"><i id="sendPrescriptionBtnLoading" class="fa fa-save"></i>ارسال به سپاس</button>
                                    </div>
                                </div>
                                <div class="card-body " id="prescreption_pagetable" style="min-height: calc(100vh - 384px);">
                                    @await Html.PartialAsync("/Modules/NewPageTable.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane pr-3 pl-3 pb-3 pt-0" id="dental" role="tabpanel">
                        <div class="row tabToggle2">
                            <div class="col-12">
                                <div class="header-title d-flex pb-4">
                                    <span>لیست دندانداکتری</span>
                                    <div class="button-items">
                                        <button type="button" id="sendDentalBtn" class="btn btn-success waves-effect"><i class="fa fa-save"></i>ارسال اطلاعات دندان</button>
                                    </div>
                                </div>
                                <div class="card-body " id="dental_pagetable" style="min-height: calc(100vh - 384px);">
                                    @await Html.PartialAsync("/Modules/NewPageTable.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane pr-3 pl-3 pb-3 pt-0" id="death" role="tabpanel">
                        <div class="row tabToggle2">
                            <div class="col-12">
                                <div class="header-title d-flex pb-4">
                                    <span>لیست فوتی</span>
                                    <div class="button-items">
                                        <button type="button" id="sendDeathBtn" class="btn btn-success waves-effect"><i class="fa fa-save"></i>ارسال اطلاعات فوتی</button>
                                    </div>
                                </div>
                                <div class="card-body " id="death_pagetable" style="min-height: calc(100vh - 384px);">
                                    @await Html.PartialAsync("/Modules/NewPageTable.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane pr-3 pl-3 pb-3 pt-0" id="medical" role="tabpanel">
                        <div class="row tabToggle2">
                            <div class="col-12">
                                <div class="header-title d-flex pb-4">
                                    <span>لیست آزمایش ها</span>
                                    <div class="button-items">
                                        <button type="button" id="sendMedicalLaboratoryBtn" class="btn btn-success waves-effect"><i class="fa fa-save"></i>ارسال اطلاعات آزمایشگاه</button>
                                    </div>
                                </div>
                                <div class="card-body " id="medicallaboratory_pagetable" style="min-height: calc(100vh - 384px);">
                                    @await Html.PartialAsync("/Modules/NewPageTable.cshtml")
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
</div>

