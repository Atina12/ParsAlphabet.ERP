@{
    ViewBag.Title = "گزارش تعرفه خدمات";
}

<div class="content-page  displaynone" id="content-page">
    <div class="content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card m-b-30 m-t-15">
                        <div class="header-title d-flex">
                            <span>@ViewBag.Title</span>
                            <div class="button-items">
                                <button id="getReport" class="btn green_2 waves-effect waves-light"><i class="fas fa-sticky-note ml-1"></i>پیش نمایش</button>
                                <button id="showReport" type="button" class="btn btn-warning"><i class="fas fa-file-import ml-1"></i>تهیه گزارش</button>
                                @await Html.PartialAsync("/Modules/_Report/_SettingReport.cshtml")
                                <button type="button" id="exportCSV" onclick="export_serviceTariff_csv()" class="btn btn-excel waves-effect"><i class="fa fa-file-excel"></i>اکسل</button>
                            </div>
                        </div>

                        <div class="card-body">
                            <fieldset class="group-box group-box-gray">
                                <button type="button" onclick="plus(this)" class="btn btn-light border-dark plus">
                                    <i class="fas fa-minus"></i>
                                </button>
                                <legend>فیلتر</legend>
                                <div class="row slideToggle open">
                                    <div class="row col-12" id="serviceTariff" data-parsley-validate>
                                        <div class="col-lg-2 col-sm-4 mb-0">
                                            <div class="form-group">
                                                <label>خروجی گزارش</label>
                                                <div>
                                                    <select class="form-control" id="insurerTypeId" tabindex="1">
                                                        <option value="1">اجباری</option>
                                                        <option value="2">تکمیلی</option>
                                                        <option value="4">طرف قرارداد</option>
                                                        <option value="5">تخفیف </option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-3 col-sm-4 mb-0">
                                            <label>از شناسه خدمت</label>
                                            <div class="col-sm-12 chosen-container-parent">
                                                <select data-placeholder="انتخاب کنید" id="fromServiceId" class="select2 select2-quick-access" tabindex="2" data-parsley-greaterthanequal="#toServiceId" data-parsley-errors-container="#fromServiceIdError">
                                                    <option value="0">انتخاب کنید</option>
                                                </select>
                                            </div>
                                            <div id="fromServiceIdError"></div>
                                        </div>
                                        <div class="form-group col-lg-3 col-sm-4 mb-0">
                                            <label>تا شناسه خدمت</label>
                                            <div class="col-sm-12 chosen-container-parent">
                                                <select data-placeholder="انتخاب کنید" id="toServiceId" class="select2 select2-quick-access" tabindex="3">
                                                    <option value="0">انتخاب کنید</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="form-group col-lg-2 col-sm-4 mb-0">
                                            <label>از نمبر تذکره خدمت</label>
                                            <div class="col-sm-12 chosen-container-parent">
                                                <select data-placeholder="انتخاب کنید" id="fromNationalCode" class="select2 select2-quick-access" tabindex="4" data-parsley-greaterthanequal="#toNationalCode" data-parsley-errors-container="#fromNationalCodeError">
                                                    <option value="0">انتخاب کنید</option>
                                                </select>
                                            </div>
                                            <div id="fromNationalCodeError"></div>
                                        </div>
                                        <div class="form-group col-lg-2 col-sm-4 mb-0">
                                            <label>تا نمبر تذکره خدمت</label>
                                            <div class="col-sm-12 chosen-container-parent">
                                                <select data-placeholder="انتخاب کنید" id="toNationalCode" class="select2 select2-quick-access" tabindex="5">
                                                    <option value="0">انتخاب کنید</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-4 mb-0">
                                            <div class="form-group">
                                                <label>نوع خدمت</label>
                                                <div>
                                                    <select class="form-control select2" id="serviceTypeId" tabindex="6"></select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-4 mb-0">
                                            <div class="form-group">
                                                <label>بیمه اجباری</label>
                                                <div class="col-sm-12 chosen-container-parent">
                                                    <select data-placeholder="انتخاب کنید" id="insurerId" required data-parsley-selectvalzero data-parsley-errors-container="#insurerIdContainer" class="select2 select2-quick-access" tabindex="7">
                                                        <option value="0">انتخاب کنید</option>
                                                    </select>
                                                </div>
                                                <div id="insurerIdContainer"></div>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-4 mb-0">
                                            <div class="form-group">
                                                <label>صندوق بیمه</label>
                                                <div class="col-sm-12 chosen-container-parent">
                                                    <select data-placeholder="انتخاب کنید" id="basicInsurerLineId" required data-parsley-selectvalzero data-parsley-errors-container="#basicInsurerLineIdContainer" class="select2 select2-quick-access" tabindex="8">
                                                        <option value="0">انتخاب کنید</option>
                                                    </select>
                                                </div>
                                                <div id="basicInsurerLineIdContainer"></div>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-4 mb-0">
                                            <div class="form-group">
                                                <label>بیمه تکمیلی</label>
                                                <div class="col-sm-12 chosen-container-parent">
                                                    <select data-placeholder="انتخاب کنید" id="compInsurerId" required data-parsley-selectvalzero data-parsley-errors-container="#compInsurerIdContainer" class="select2 select2-quick-access" tabindex="9">
                                                        <option value="0">انتخاب کنید</option>
                                                    </select>
                                                </div>
                                                <div id="compInsurerIdContainer"></div>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-4 mb-0">
                                            <div class="form-group">
                                                <label>صندوق بیمه تکمیلی</label>
                                                <div class="col-sm-12 chosen-container-parent">
                                                    <select data-placeholder="انتخاب کنید" id="compInsurerLineId" required data-parsley-selectvalzero data-parsley-errors-container="#compInsurerLineIdContainer" class="select2 select2-quick-access" tabindex="10">
                                                        <option value="0">انتخاب کنید</option>
                                                    </select>
                                                </div>
                                                <div id="compInsurerLineIdContainer"></div>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-4 mb-0">
                                            <div class="form-group">
                                                <label>طرف قرارداد</label>
                                                <div class="col-sm-12 chosen-container-parent">
                                                    <select data-placeholder="انتخاب کنید" id="thirdPartyId" required data-parsley-selectvalzero data-parsley-errors-container="#thirdPartyIdContainer" class="select2 select2-quick-access" tabindex="11">
                                                        <option value="0">انتخاب کنید</option>
                                                    </select>
                                                </div>
                                                <div id="thirdPartyIdContainer"></div>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-4 mb-0">
                                            <div class="form-group">
                                                <label>تخفیف</label>
                                                <div class="col-sm-12 chosen-container-parent">
                                                    <select data-placeholder="انتخاب کنید" id="discountId" required data-parsley-selectvalzero data-parsley-errors-container="#discountIdContainer" class="select2 select2-quick-access" tabindex="12">
                                                        <option value="0">انتخاب کنید</option>
                                                    </select>
                                                </div>
                                                <div id="discountIdContainer"></div>
                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-sm-4 mb-0">
                                            <label>وضعیت خدمت</label>
                                            <div class="funkyradio funkyradio-success" onkeydown="funkyradio_keydown(event,'serviceStatus');" tabindex="13">
                                                <input id="serviceStatus" type="checkbox" onchange="funkyradio_onchange(this)" switch-value="فعال,غیرفعال"/>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                            <fieldset class="group-box group-box-gray mt-3">
                                <legend>پیش نمایش</legend>
                                <div class="pagetable-report" id="mainReport">
                                    @await Html.PartialAsync("/Modules/PageTableReport.cshtml")
                                </div>
                            </fieldset>

                        </div>
                    </div>
                </div>
            </div>
        </div> <!-- end col -->
    </div>
</div>


<!--Modal-->
<div id="previewReportModal" class="modal fade" data-keyboard="false" role="dialog" aria-labelledby="modal_title" aria-hidden="true" onkeydown="modal_onkeydown(event, this)">
    <div class="modal-dialog modal-size-md">
        <div class="modal-content">
            <div class="modal-header bg-header">
                <div class="text-right">
                    <h5 class="modal-title mt-0" id="modal_title">تهیه گزارش</h5>
                </div>
            </div>
            <div class="modal-body modal-max-height text-right" data-parsley-validate>
                <div class="col-lg-6 col-sm-12 pr-0">
                    <div class="form-group">
                        <label>شکل گزارش</label>
                        <select class="form-control" id="reportType" tabindex="20">
                            <option value="101" selected>صورت خلاصه</option>
                            <option value="102">صورت ریز</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button id="modal-previewReport" class="btn btn-success waves-effect waves-light" tabindex="21"><i class="fa fa-save"></i>تهیه گزارش</button>
                <button id="modal-close" onclick="modal_closePreviewRepor('previewReportModal')" class="btn btn-secondary waves-effect" title="Esc"><i class="fa fa-times-circle"></i>انصراف</button>
            </div>
        </div>
    </div>
</div>
<!--Modal-->


<script src="~/js/formplate.js"></script>
