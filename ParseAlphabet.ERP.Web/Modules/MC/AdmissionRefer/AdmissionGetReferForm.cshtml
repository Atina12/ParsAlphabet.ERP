<fieldset class="group-box group-box-orange mt-3" id="sendReferFieldset" data-parsley-validate>
    <legend>ارسال ارجاع</legend>
    <div class="row " id="referForm" data-parsley-validate>
        <fieldset class="group-box group-box-gray fieldset-lab" style="width: 100%; margin: 15px; border: .5px dashed;">
            <div class="row col-12">
                <div class="form-group col-lg-1  col-sm-2">
                    <label> قابل مشاهده</label>
                    <div class="funkyradio funkyradio-success" onkeydown="funkyradio_keydown(event,'iSQueriable');" tabindex="10">
                        <input id="iSQueriable" type="checkbox" onchange="funkyradio_onchange(this)" switch-value="بلی,خیر"/>
                    </div>
                </div>
                <div class="form-group col-lg-2  col-sm-3">
                    <label> دلیل ارجاع</label>
                    <div>
                        <select id="referredReasonId" class="form-control select2" tabindex="11" required data-parsley-selectvalzero data-parsley-errors-container="#referredReasonIdContainer">
                            <option value="0">انتخاب کنید</option>
                        </select>
                    </div>
                    <div id="referredReasonIdContainer"></div>
                </div>
                <div class="form-group col-lg-2  col-sm-2">
                    <label>نوع ارجاع</label>
                    <div>
                        <select id="referredTypeId" class="form-control select2" tabindex="12" required data-parsley-selectvalzero data-parsley-errors-container="#referredTypeIdContainer">
                            <option value="0">انتخاب کنید</option>
                        </select>
                    </div>
                    <div id="referredTypeIdContainer"></div>
                </div>
                <div class="form-group col-lg-2  col-sm-2">
                    <label>تاریخ ارجاع</label>
                    <div>
                        <input id="referredDateTime" type="text" class="form-control" @*required*@ data-parsley-shamsidatetime maxlength="16" placeholder="____/__/__ __:__" data-inputmask="'mask':'9999/99/99 99:99'">

                    </div>
                    <div id="referredTypeIdContainer"></div>
                </div>
                <div class="form-group col-lg-5  col-sm-5">
                    <label>توضیحات</label>
                    <div>
                        <input id="referralDescription" type="text" class="form-control" required maxlength="100" tabindex="13"/>
                    </div>
                </div>
            </div>
        </fieldset>
        <fieldset class="group-box group-box-gray fieldset-lab" style="width: 100%; margin: 15px; margin-top:0px; border: .5px dashed;">
