<!--Modal-->
<div id="AddEditModalCu" class="modal fade" tabindex="-1" data-keyboard="false" role="dialog" aria-labelledby="modal_title" aria-hidden="true" onkeydown="modal_onkeydown(event, this)">
<div class="modal-dialog modal-size-lg">
<div class="modal-content">
<div class="modal-header bg-header">
    <div class="text-right">
        <h5 class="modal-title mt-0" id="modal_title">افزودن ویرایش</h5>
    </div>
</div>


<div class="modal-body modal-max-height text-right" data-parsley-validate>
<ul class="nav nav-pills nav-justified" role="tablist">
    <li class="nav-item waves-effect waves-light" onclick="focusInput(1)">
        <a class="nav-link active" data-toggle="tab" href="#personalBoxCU" role="tab" id="firstTab">
            <span class="d-none d-md-block">اطلاعات فردی</span>
        </a>
    </li>
    <li class="nav-item waves-effect waves-light" onclick="focusInput(2)">
        <a class="nav-link" data-toggle="tab" href="#callBoxCU" role="tab">
            <span class="d-none d-md-block">تماس</span>
        </a>
    </li>
    <li class="nav-item waves-effect waves-light" onclick="focusInput(3)">
        <a class="nav-link" data-toggle="tab" href="#vatBoxCU" role="tab">
            <span class="d-none d-md-block">مالیاتی</span>
        </a>
    </li>
    <li class="nav-item waves-effect waves-light" onclick="focusInput(4)">
        <a class="nav-link" data-toggle="tab" href="#insuBoxCU" role="tab">
            <span class="d-none d-md-block">بیمه</span>
        </a>
    </li>
</ul>

<div class="tab-content group-box group-box-gray text-right">
<div class="tab-pane active p-3 " id="personalBoxCU" role="tabpanel" data-parsley-validate>
    <div class="form-group row tabToggle1">
        <div class="form-group col-sm-12">
            <div class="row" id="rowKeyId">
                <div class="d-block w-100 text-right mb-2 pr-3">
                    <div class="key_id">
                        <lable id="modal_keyid_caption"></lable>
                        <span id="modal_keyid_value"></span>
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>نوع شخصیت</label>
            <div>
                @*oninput="checkCustomerInput()"*@
                <select id="partnerTypeIdCu" class="form-control" tabindex="1" required>
                    <option value="1">حقیقی</option>
                    <option value="2">حقوقی</option>
                </select>
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>گروه مشتری</label>
            <div>
                <select id="personGroupIdCu" class="form-control select2" tabindex="2" requirerd data-parsley-selectvalzero onchange="checkCustomerInput()" data-parsley-errors-container="#personGroupIdCuErrorContainer">
                    <option value="0">انتخاب نمایید</option>
                </select>
            </div>
            <div id="personGroupIdCuErrorContainer"></div>
        </div>
        <div class="form-group col-sm-4">
            <label>صنعت</label>
            <div>
                <select id="industryIdCu" class="form-control select2" tabindex="3">
                    <option value="0">انتخاب کنید</option>
                </select>
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>عنوان</label>
            <div>
                <select id="personTitleIdCu" class="form-control select2" title="" tabindex="4">
                    <option value="0">انتخاب نمایید</option>
                </select>
            </div>
        </div>
        <div class="form-group col-sm-4 indiv">
            <label>نام</label>
            <div>
                <input id="firstNameCu" type="text" class="form-control atoz-valid" oninput="checkCustomerInput()" required maxlength="50" title="" tabindex="5">
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label id="partnerNameCu">نام شرکت</label>
            <div>
                <input id="lastNameCu" type="text" class="form-control" oninput="checkCustomerInput()" required maxlength="50" title="" tabindex="6">
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>نام نماینده</label>
            <div>
                <input id="agentFullNameCu" type="text" class="form-control atoz-valid" maxlength="50" title="" tabindex="7">
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>نام تجاری</label>
            <div>
                <input id="brandNameCu" type="text" class="form-control" maxlength="50" title="" tabindex="8">
            </div>
        </div>
        <div class="form-group col-sm-4 indiv">
            <label>جنسیت</label>
            <div>
                <select id="genderIdCu" class="form-control" title="" oninput="checkCustomerInput()" required tabindex="9">
                    <option value="1">مرد</option>
                    <option value="2">زن</option>
                </select>
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>نمبر تذکره</label>
            <div>
                <input id="nationalCodeCu" type="text" class="form-control str-number" title="" tabindex="10" oninput="checkCustomerInput()" data-parsley-existnationalcode=""/>
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>تاریخ</label>
            <div>
                <input id="idDatePersianCu" type="text" class="form-control" placeholder="____/__/__" data-inputmask="'mask':'9999/99/99'" data-parsley-shamsidate maxlength="10" title="" tabindex="11">
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>شماره</label>
            <div>
                <input id="idNumberCu" type="text" class="form-control str-number" maxlength="10" title="" tabindex="12">
            </div>
        </div>
        <div class="form-group col-sm-4" id="jobTitleContainerCu">
            <label>عنوان شغلی</label>
            <div>
                <input id="jobTitleCu" type="text" class="form-control" maxlength="50" title="" tabindex="13">
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>وضعیت</label>
            <div class="funkyradio funkyradio-success" onkeydown="funkyradio_keydown(event,'isActiveCu');" tabindex="14">
                <input id="isActiveCu" type="checkbox" name="checkbox" onchange="funkyradio_onchange(this)" switch-value="فعال,غیرفعال"/>
            </div>
        </div>

    </div>
</div>
<div class="tab-pane p-3" id="callBoxCU" role="tabpanel">
    <div class="form-group row tabToggle2" data-parsley-validate>
        <div class="form-group col-sm-4">
            <label>کشور</label>
            <div>
                <select id="locCountryIdCu"
                        class="form-control select2"
                        oninput="checkCustomerInput()"
                        required
                        tabindex="15"
                        data-parsley-errors-container="#locCountryIdCuError">
                </select>
            </div>
            <div id="locCountryIdCuError"></div>
        </div>
        <div class="form-group col-sm-4">
            <label>ولایت</label>
            <div>
                <select id="locStateIdCu" class="form-control  select2" tabindex="16" data-parsley-errors-container="#locStateIdCuError">
                </select>
            </div>
            <div id="locStateIdCuError"></div>
        </div>
        <div class="form-group col-sm-4">
            <label>شهر </label>
            <div>
                <select id="locCityIdCu" class="form-control select2" tabindex="17" data-parsley-errors-container="#locCityIdCuError">
                </select>
            </div>
            <div id="locCityIdCuError"></div>
        </div>
        <div class="form-group col-sm-4">
            <label>نمبر خانه (Postal Code)</label>
            <div>
                <input id="postalCodeCu" type="text" class="form-control str-number" maxlength="10" title="" tabindex="18">
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>شماره تلفن ثابت</label>
            <div>
                <input id="phoneNoCu" type="text" class="form-control str-number" data-parsley-phoneno title="" data-parsley-errors-container="#phoneNoCuCodeError" maxlength="11" tabindex="19"/>
            </div>
            <div id="phoneNoCuCodeError"></div>
        </div>
        <div class="form-group col-sm-4">
            <label>شماره موبایل</label>
            <div>
                <input id="mobileNoCu" type="text" class="form-control str-number" data-parsley-mobileno title="" data-parsley-errors-container="#mobileNoCuCodeError" maxlength="11" tabindex="20"/>
            </div>
            <div id="mobileNoCuCodeError"></div>
        </div>
        <div class="form-group col-sm-4">
            <label>ایمیل</label>
            <div>
                <input id="emailCu"
                       type="email"
                       class="form-control"
                       title=""
                       maxlength="50"
                       data-parsley-email
                       tabindex="21"/>
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>آدرس وب سایت</label>
            <div>
                <input id="webSiteCu" type="text"
                       class="form-control"
                       maxlength="50"
                       title=""
                       tabindex="22">
            </div>
        </div>
        <div class="form-group col-sm-8">
            <label>آدرس</label>
            <div>
                <input id="addressCu"
                       type="text"
                       class="form-control"
                       maxlength="100"
                       title=""
                       tabindex="23">
            </div>
        </div>
    </div>
</div>
<div class="tab-pane p-3" id="vatBoxCU" role="tabpanel">
    <div class="form-group row tabToggle3" data-parsley-validate>
        <div class="form-group col-sm-4 ">
            <label>مشمول مالیات بر ارزش افزوده</label>
            <div class="funkyradio funkyradio-success" onkeydown="funkyradio_keydown(event,'vatIncludeCu');" tabindex="24">
                <input id="vatIncludeCu" type="checkbox" onchange="funkyradio_onchange(this)" switch-value="بلی,خیر"/>
            </div>
        </div>
        <div class="form-group col-sm-4">
            <label>VATمنطقه</label>
            <div>
                <select id="vatAreaIdCu" class="form-control" tabindex="25" data-parsley-errors-container="#vatAreaIdVeErrorContainer">
                    <option value="0">انتخاب نمایید</option>
                </select>
            </div>
            <div id="vatAreaIdVeErrorContainer"></div>
        </div>
        <div class="form-group col-sm-4">
            <label>اعتبار مالیات بر ارزش افزوده</label>
            <div class="funkyradio funkyradio-success" onkeydown="funkyradio_keydown(event,'vatEnableCu');" tabindex="26">
                <input id="vatEnableCu" type="checkbox" onchange="funkyradio_onchange(this)" switch-value="دارد,ندارد"/>
            </div>
        </div>
        <div class="form-group col-sm-4 comp">
            <label>کداقتصادی</label>
            <div>
                <input id="taxCodeCu" type="text" class="form-control" maxlength="12" title="" tabindex="27">
            </div>
        </div>
    </div>
</div>
<div class="tab-pane p-3" id="insuBoxCU" role="tabpanel">
    <div class="form-group row tabToggle4" data-parsley-validate>
        <div class="form-group col-sm-4">
            <label>بیمه گر</label>
            <div class="col-sm-12 chosen-container-parent">
                <select id="insurerIdCu" data-placeholder="انتخاب کنید" class="form-control select2" tabindex="28"
                        data-parsley-errors-container="#insurerIdErrorContainer" oninput="checkCustomerInput()">
                    <option value="0" selected>انتخاب کنید</option>
                </select>
            </div>
            <div id="insurerIdErrorContainer"></div>
        </div>
        <div class="form-group col-sm-4">
            <label>شماره بیمه</label>
            <div>
                <input id="insuranceNoCu" type="text" class="form-control str-number" title="" maxlength="16" tabindex="29"
                       required
                       oninput="checkCustomerInput()"
                       data-parsley-existinsuranceno=""
                       data-parsley-validate-if-empty="true"
                       data-parsley-errors-container="#insuranceNoCuErrorContainer"/>
            </div>
            <div id="insuranceNoCuErrorContainer"></div>
        </div>
    </div>
</div>
</div>

</div>
<div class="modal-footer">
    <button id="modal-save" onclick="modal_save();" class="btn btn-success waves-effect waves-light" tabindex="34" title="Ctrl + S"><i class="fa fa-save"></i>ذخیره</button>
    <button id="modal-close" onclick="modal_close(`AddEditModalCu`)" class="btn btn-secondary waves-effect" title="Esc"><i class="fa fa-times-circle"></i>انصراف</button>
</div>
</div>
</div>
</div>
<!--Modal-->

<script src="/Modules/SM/Customer/CustomerAddEdit.js"></script>